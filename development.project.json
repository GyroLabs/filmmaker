{
	"name": "filmmaker",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"**/LICENSE.txt"
	],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
      "Filmmaker": {
        "$path": "out",
        "include": {
          "$path": "include",
          "node_modules": {
            "$path": "node_modules/@rbxts"
          }
        }
      }
    }
	}
}
